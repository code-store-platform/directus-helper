{
	"name": "helper",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start": "node ./dist/cli.js",
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"test:w": "ava --watch"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"chokidar": "^3.6.0",
		"clipboardy": "^4.0.0",
		"deep-equal": "^2.2.3",
		"ink": "^4.1.0",
		"ink-link": "^3.0.0",
		"ink-select-input": "^5.0.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@directus/sdk": "^14.0.0",
		"@directus/types": "^11.0.3",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/chokidar": "^2.1.3",
		"@types/deep-diff": "^1.0.5",
		"@types/deep-equal": "^1.0.4",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"tsimp": "^2.0.11",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsimp"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
